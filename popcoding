import java.util.*;
  
  public class Main{
  
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int cout = 0;
    while(n > 0){
       int rem  = n % 10;

        n = n  / 10;
        cout++;
    }
    System.out.println(cout);
   }
  }
